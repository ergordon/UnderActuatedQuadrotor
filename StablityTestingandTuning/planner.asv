function xe = planner(t, x)

q_desired = zeros(12,1);
q = x(1:3);
b_att = 3;
k_att = 3;
b_des = .01;
k_des = .01;

q_goal = [0;0;-1];

if norm(q-q_goal) <= b_att
    gradf = k_att*norm(q-q_goal);
else
    gradf = k_att*b_att*(q-q_goal)/norm(q-q_goal);
end

if norm(k_des*gradf) <= b_des
    q_desired(1:3) = q - k_des*gradf;
else
    q_desired(1:3) = q - b_des * gradf/norm(gradf);
end

xe = q_desired;
fprintf('%2.5f \t%2.5f \t%2.5f \t%2.5f\n',t,xe(1),xe(2),xe(3))

